{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"isolatedModules": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"noEmit": true,
		"allowJs": false,
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src/**/*.ts", "src/**/*.tsx", "remix.env.d.ts", "types.d.ts"]
}
